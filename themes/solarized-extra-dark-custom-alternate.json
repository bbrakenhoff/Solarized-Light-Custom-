	"tokenColors": [
		{
			"settings": {
				"foreground": "#657b83ff",
				"background": "#fdf6e3ff"
			}
        },
        {
			"name": "Comment",
			"scope": [
				"comment",
				"comment.block.documentation.javadoc"
			],
			"settings": {
				"foreground": "#93a1a1"
			}
        },
        {
			"name": "HTML/XML: Attribute",
			"scope": [
				"entity.other.attribute-name.html",
				"entity.other.attribute-name.xml",
				"entity.other.attribute-name.localname.xml"
			],
			"settings": {
				"foreground": "#b58900"
			}
        },
        {
			"name": "HTML/XML: Attribute value",
			"scope": [
				"string.quoted.double.html",
				"string.quoted.double.xml"
			],
			"settings": {
				"foreground": "#2aa198"
			}
		},
		{
			"name": "HTML/XML: Content",
			"scope": [
				"text.html.basic",
				"text.xml"
			],
			"settings": {
				"foreground": "#cb4b16"
			}
        },
        {
			"name": "HTML/XML: DOCTYPE",
			"scope": "meta.tag.sgml.doctype.html",
			"settings": {
				"foreground": "#d33682"
			}
		},
		{
			"name": "HTML/XML: Tag",
			"scope": [
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.definition.tag.xml",
				"meta.tag.other.html",
				"meta.tag.any.html"
			],
			"settings": {
				"foreground": "#657b83"
			}
		},
		{
			"name": "HTML/XML: Tag name",
			"scope": [
				"punctuation.definition.tag.name.html",
				"punctuation.definition.tag.name.xml",
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#268bd2"
			}
		},

		{
			"name": "CSS/SCSS: Tag",
			"scope": [
				"entity.name.tag.css",
				"entity.name.tag.custom.scss"
			],
			"settings": {
				"foreground": "#b58900"
			}
		},
		{
			"name": "CSS/SCSS: Property name",
			"scope": [
				"support.type.property-name.css",
				"support.type.property-name.scss"
			],
			"settings": {
				"foreground": "#859900"
			}
		},
		{
			"name": "CSS/SCSS: Class",
			"scope": "entity.other.attribute-name.class, punctuation.definition.entity.css, entity.other.attribute-name.parent-selector-suffix.scss",
			"settings": {
				"foreground": "#268bd2"
			}
		},
		{
			"name": "CSS/SCSS: Unit",
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#657b83",
				"fontStyle": "normal"
			}
		},
		{
			"name": "SCSS: Entity reference",
			"scope": "entity.name.tag.reference.scss",
			"settings": {
				"foreground": "#073642"
			}
		},
		{
			"name": "CSS/SCSS: Function call",
			"scope": "support.function.css, support.function.misc.scss, entity.name.function.scss",
			"settings": {
				"foreground": "#cb4b16"
			}
		},
		{
			"name": "SCSS: Variable",
			"scope": "variable.scss",
			"settings": {
				"foreground": "#6c71c4"
			}
		},
		{
			"name": "CSS/SCSS: Id",
