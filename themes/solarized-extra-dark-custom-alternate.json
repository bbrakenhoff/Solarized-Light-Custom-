			"settings": {
				"fontStyle": "bold",
				"foreground": "#93a1a1"
			}
		},
		{
			"name": "JS Doc: Param type",
			"scope": "entity.name.type.instance.jsdoc",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Instance field",
