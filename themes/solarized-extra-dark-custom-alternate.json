			"settings": {
				"foreground": "#268bd2",
				"fontStyle": " bold"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#6c71c4"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword, keyword.operator.static, keyword.operator.new, keyword.operator.this, keyword.operator.expression, keyword.operator.delete, storage.type, storage.modifier, storage.arrow, variable.language.this, constant.language.boolean, constant.language.null, constant.language.json, constant.language.java, variable.language.java, constant.language.php",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d33682"
			}
		},
        {
			"name": "Braces, brackets, operators",
			"scope": "punctuation.section, keyword.operator.assignment, keyword.operator.arithmetic, keyword.operator.ternary, punctuation.terminator, punctuation.definition.block, punctuation.accessor, keyword.operator.dereference.java, punctuation.definition.parameters, meta.brace.round.js, meta.brace.square.js, meta.method.identifier.java, keyword.operator.increment-decrement.java, keyword.control.ternary.java, keyword.operator.comparison, keyword.operator.relational, keyword.operator.logical, keyword.operator.css, keyword.operator.class.php, keyword.operator.key.php",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#93a1a1"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator.arithmetic",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#657b83"
			}
		},
		{
			"name": "Class",
			"scope": "entity.name.type.class, storage.type.java, support.class.php, support.other.namespace.use-as.php, entity.other.inherited-class.php, meta.other.inherited-class.php, entity.other.inherited-class.ts",
			"settings": {
				"foreground": "#859900",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Abstract class + interface",
			"scope": "entity.name.type.interface.php",
			"settings": {
				"foreground": "#859900",
				"fontStyle": "italic"
			}
		},
		{
			"name": "String",
			"scope": "string.quoted.single, string.quoted.double",
			"settings": {
				"foreground": "#2aa198"
			}
		},
		{
			"name": "CSS/SCSS: Pseudo element/class",
			"scope": "entity.other.attribute-name.pseudo-element, entity.other.attribute-name.pseudo-class",
			"settings": {
				"foreground": "#b58900",
				"fontStyle": " italic"
			}
		},
		{
			"name": "JS: Function name",
			"scope": "support.function, entity.name.function",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#268bd2"
			}
		},
		{
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": " italic"
            }
