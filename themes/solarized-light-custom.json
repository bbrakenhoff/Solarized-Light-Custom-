			"settings": {
				"foreground": "#859900",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Abstract class + interface",
			"scope": "entity.name.type.interface.php",
			"settings": {
				"foreground": "#859900",
				"fontStyle": "italic"
			}
		},
		{
			"name": "String",
			"scope": "string.quoted.single, string.quoted.double",
			"settings": {
				"foreground": "#2aa198"
			}
