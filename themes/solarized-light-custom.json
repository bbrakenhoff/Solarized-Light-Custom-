				"fontStyle": "normal"
			}
		},
		{
			"name": "SCSS: Entity reference",
			"scope": "entity.name.tag.reference.scss",
			"settings": {
				"foreground": "#073642"
			}
		},
		{
			"name": "CSS/SCSS: Function call",
			"scope": "support.function.css, support.function.misc.scss, entity.name.function.scss",
			"settings": {
				"foreground": "#cb4b16"
			}
		},
		{
			"name": "SCSS: Variable",
			"scope": "variable.scss",
			"settings": {
				"foreground": "#6c71c4"
			}
		},
		{
			"name": "CSS/SCSS: Id",
			"scope": "entity.other.attribute-name.id",
			"settings": {
				"foreground": "#268bd2",
				"fontStyle": " bold"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#6c71c4"
			}
		},
		{
			"name": "Keyword",
