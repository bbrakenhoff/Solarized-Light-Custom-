			"settings": {
				"fontStyle": "bold",
				"foreground": "#d33682"
			}
		},
		{
			"name": "Braces, brackets",
			"scope": "punctuation.section, keyword.operator.assignment, keyword.operator.ternary, punctuation.terminator, punctuation.definition.block, punctuation.accessor, keyword.operator.dereference.java, punctuation.definition.parameters, meta.brace.round, meta.brace.square, meta.method.identifier.java, keyword.operator.increment-decrement.java, keyword.control.ternary.java, keyword.operator.comparison, keyword.operator.relational, keyword.operator.logical, keyword.operator.css, keyword.operator.class.php, keyword.operator.key.php, punctuation.definition.parameters.begin.ts, punctuation.definition.parameters.end.ts",
