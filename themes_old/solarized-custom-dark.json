        {
            "name": "functionArguments",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "keywords",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier",
                "constant.language.boolean",
                "variable.language.this",
