    "tokenColors": [
        {
            "name": "comments",
            "scope": [
                "comment",
                "comment.block.documentation",
                "punctuation.definition.comment",
                "punctuation.definition.comment.php"
            ],
            "settings": {
            }
        },
        {
            "settings": {
                "background": "#fdf6e3ff",
                "foreground": "#657b83ff"
            }
        },
        {
            "name": "abstractClassesInterfaces",
            "scope": [
                "entity.name.type.interface"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#859900"
            }
        },
        {
            "name": "bracesParenthesesOperators",
            "scope": [
                "punctuation.definition",
                "meta.brace.round.ts",
                "meta.brace.square.ts"
            ],
            "settings": {
                "fontStyle": "normal",
                "foreground": "#93a1a1"
            }
        },
        {
            "name": "classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "storage.type.java"
            ],
            "settings": {
                "fontStyle": "normal",
                "foreground": "#859900"
            }
        },
        {
            "name": "commentTags",
            "scope": [
                "storage.type.class.jsdoc",
                "punctuation.definition.block.tag.jsdoc"
